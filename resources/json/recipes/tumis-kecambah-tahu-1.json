        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Goreng tahu yang sudah di potong dadu hingga setengah matang.",
                "image_url": "content-images\/images\/20250516\/0FXACbHdTLNo3LbpMzmnkCqlMVgp9d7k-31373437333634383738d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
