        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Tuang tepung terigu dan tapioka.",
                "image_url": "content-images\/images\/20240612\/coDURaIphYplnyQvibF8aqoAOICyA5p9-31373138313535393736d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
