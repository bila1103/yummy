        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Potoh dadu tahu putih, sisihkan.",
                "image_url": "content-images\/images\/20250416\/gIclmKBrYotvYwI5K6VY3391gYD2WUpq-31373434373733343535d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
