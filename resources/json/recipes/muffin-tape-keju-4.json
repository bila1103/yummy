        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Campur semua bahan bahan kering aduk rata dan sisihkan terlebih dahulu.",
                "image_url": "content-images\/images\/20250424\/rJd9u2qawOdSxzSj2HHGzOt6eXQN5y6w-31373435343833343439d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
