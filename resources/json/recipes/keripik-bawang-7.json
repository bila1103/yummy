            },
            {
                "title": "Langkah 4",
                "text": "Lalu potong lebar-lebar.",
                "image_url": "content-images\/images\/20250415\/2ZIybuPCG5Cr8xpfhgdr3KEs9Qfaq5Uw-31373434373139303133d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
