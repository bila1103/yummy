        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Potong-potong lobak, wortel, sawi putih, dan tahu putih.",
                "image_url": "content-images\/images\/20250314\/bZrK9pLFiVsKddSoOAOEVF5wrDFE6n0n-31373431393630353938d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
