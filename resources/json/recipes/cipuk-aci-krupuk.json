        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Hancurkan kerupuk warna warni atau krupuk seblak.",
                "image_url": "content-images\/images\/20240927\/jITYqCXTF85sOdasrSYs853bbMGy4Ip6-31373237343339383133d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
