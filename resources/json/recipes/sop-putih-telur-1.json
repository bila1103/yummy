        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Siapkan bahan-bahannya. Sisihkan.",
                "image_url": "content-images\/images\/20250507\/1uhimWFn2Gek6saMDWrKzs8wqVUKPbQB-31373436353930363230d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
