            },
            {
                "title": "Langkah 6",
                "text": "Goreng sampai matang lalu tiriskan",
                "image_url": "content-images\/images\/20250427\/4BNNKLobyD6gZaAOWQDXPSPuDESIIKo8-31373435373231303237d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
