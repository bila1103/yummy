            },
            {
                "title": "Langkah 6",
                "text": "Tuang air perlahan lahan.",
                "image_url": "content-images\/images\/20250312\/suzMrscfOdqtpfNLGGbg12CCqTJJSuTA-31373431373731373438d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
