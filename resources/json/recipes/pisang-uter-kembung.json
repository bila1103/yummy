            },
            {
                "title": "Langkah 4",
                "text": "Tambahkan air dan aduk rata semua adonan",
                "image_url": "content-images\/images\/20250312\/yVT00DdNDankSyAjUbAWRgAzk4aH0P4l-31373431373539353337d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
