        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Bersihkan ikan, potong bagian kepalanya.",
                "image_url": "content-images\/images\/20250403\/4ScuZJwebELJgaEyltIyM3uAbxqpC3tU-31373433363730333339d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
