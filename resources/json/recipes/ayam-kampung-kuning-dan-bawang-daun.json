            },
            {
                "title": "Langkah 5",
                "text": "Masukan irisan bawang daun, kecilkan api",
                "image_url": "content-images\/images\/20250427\/6AzymxOwiWUdaNWsZA8BrqvJ6p7I4kMN-31373435373236363638d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
