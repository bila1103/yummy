        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Cuci bersih cumi-cumi, jangan buang kantung tintanya.",
                "image_url": "content-images\/images\/20250418\/xCjAzsOWdP3xPezum6QQpVKJOC7BEp7T-31373434393837353234d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
