        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Campur semua bahan utama, marinasi selama 20 menit.",
                "image_url": "content-images\/images\/20250506\/0MrxSdduHw4zXLwBgNZ3Pk6cu1SQXhEI-31373436353330343132d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
