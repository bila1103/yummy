            },
            {
                "title": "Langkah 5",
                "text": "Masukkan ati ampela aduk rata",
                "image_url": "content-images\/images\/20250520\/f0e115Wu9ER3lXnvp7ifraljxrzn4mao-31373437373533303639d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
