        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Iris tempe berbentuk korek api.",
                "image_url": "content-images\/images\/20240221\/hh5BRuqPPRcqTF1qg5g3SMNk0rb1m9zq-31373038343736353131d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
