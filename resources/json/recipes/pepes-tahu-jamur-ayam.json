            },
            {
                "title": "Langkah 2",
                "text": "Haluskan tahu",
                "image_url": "content-images\/images\/20250305\/93BZ4ZKk4YFePfBxkYtYwgSXJvrHzKdU-31373431313633353932d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
