            },
            {
                "title": "Langkah 4",
                "text": "Aduk merata.",
                "image_url": "content-images\/images\/20250305\/5ttVteuXFAHP1jFEnPG2Vhhlc0uHWNmu-31373431313633363632d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
