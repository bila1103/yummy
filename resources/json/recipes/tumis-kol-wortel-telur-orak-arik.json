        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Kocok lepas telur, lalu masak orak arik, angkat, sisihkan.",
                "image_url": "content-images\/images\/20250420\/EbN0agR2ZEiVfBNCsxxGNMF9P0fQzsI1-31373435313235353136d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
