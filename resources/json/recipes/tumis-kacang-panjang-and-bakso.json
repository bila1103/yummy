            },
            {
                "title": "Langkah 4",
                "text": "Masukkan air.",
                "image_url": "content-images\/images\/20240626\/9JjXSsgfzpzLJMaEM7s3RRqguKykxgEN-31373139343130363133d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
