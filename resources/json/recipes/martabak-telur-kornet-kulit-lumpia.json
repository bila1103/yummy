        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Potong dadu kornet olahan,lalu tumis hingga matang,angkat.",
                "image_url": "content-images\/images\/20250416\/GKGT9XFbzVTthlyWsVqafhSZzucdrLRY-31373434383132373632d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
