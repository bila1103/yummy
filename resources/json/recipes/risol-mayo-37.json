        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Masukkan tepung terigu.",
                "image_url": "content-images\/images\/20240611\/OT9OLF4xBmlSnmOmM6MrAYAFBYXxQL7L-31373138303831313036d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
