        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Halus kacang tanah yang sudah di sangrai",
                "image_url": "content-images\/images\/20250425\/00xjY5xgqjXU7UmsIA96fBbWdDBV67zm-31373435353838343233d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
