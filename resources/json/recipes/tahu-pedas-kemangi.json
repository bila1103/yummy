        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Cuci dan potong-potong tahu sesuai selera",
                "image_url": "content-images\/images\/20250518\/vDC0It6K8umem6hhuq4R7zMk5HaQqNer-31373437353730313439d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
