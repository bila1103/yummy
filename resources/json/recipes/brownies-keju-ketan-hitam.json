        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Masukkan bahan A kecuali krimer kental manis putih ke dalam panci.",
                "image_url": "content-images\/images\/20250411\/h6c5EAHh2972DaWy3BwaykN4OHb8WOum-31373434333339353634d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
