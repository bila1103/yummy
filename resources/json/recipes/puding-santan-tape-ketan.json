        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Campur jadi satu agar-agar bubuk, gula pasir, dan santan.",
                "image_url": "content-images\/images\/20240623\/Q71DCirrKFuMhvsbQsMZIYZApcXgOLRq-31373139313131323832d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
