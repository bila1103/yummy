            },
            {
                "title": "Langkah 2",
                "text": "Tambahkan 1 liter air, aduk rata.",
                "image_url": "content-images\/images\/20250422\/Cvh2nq7oS1doDNXOH4qY9QJOhaUhxl5d-31373435333330303931d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
