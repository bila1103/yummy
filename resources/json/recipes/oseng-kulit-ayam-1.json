            },
            {
                "title": "Langkah 4",
                "text": "Masukkan dan goreng kol.",
                "image_url": "content-images\/images\/20250114\/sK2bNZwHPmT6Cn28oTL33yBe0x1CQ4cl-31373336383338303132d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
