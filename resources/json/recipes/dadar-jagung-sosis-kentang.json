            },
            {
                "title": "Langkah 6",
                "text": "Tambahkan sedikit air jika dirasa adonan terlalu kering",
                "image_url": "content-images\/images\/20250508\/4RPl2K4cl1NCBNxugt0y3i1wt3PfmCcs-31373436373133393636d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
