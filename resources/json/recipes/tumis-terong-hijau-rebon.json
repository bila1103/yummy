        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Panaskan sedikit minyak, masak rebon hingga setengah kering.",
                "image_url": "content-images\/images\/20250421\/WtID9jBIBMTU1KhQIxraI2lbLTFaT3NM-31373435323533313739d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
