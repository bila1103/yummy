        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Bersihkan bandeng, dan marinasi dengan air jeruk nipis sekitar 5 menit. Lalu bilas.",
                "image_url": "content-images\/images\/20250416\/HEi6GoH0ZnC7GKjsqPHhudhGI2kL8FBG-31373434383033343534d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
