        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Langkah pertama, siapkan buah strawberry. Tusuk-tusuk menggunakan tusukan sate.",
                "image_url": "content-images\/images\/20250421\/hN3uOWcqrdaJihyq0wihT815LBOVTggP-31373435323133383336d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
