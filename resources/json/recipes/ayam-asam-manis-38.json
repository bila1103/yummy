            },
            {
                "title": "Langkah 3",
                "text": "Goreng ayam hingga matang",
                "image_url": "content-images\/images\/20250118\/LhLqtYz4td1Xb93s8dv6jL2kHxfBNqzL-31373337313733313335d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
                "order": 3,
