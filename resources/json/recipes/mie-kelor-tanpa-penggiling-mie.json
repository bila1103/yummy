        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Blender daun kelor dan air hingga lembut.",
                "image_url": "content-images\/images\/20250418\/iTdzuyNHaM9MM8IMh7XDX6Rp8ftIlNau-31373434393433313832d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
