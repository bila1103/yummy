        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Siapkan biskuit lalu masukkan ke dalam plastik prapatan.",
                "image_url": "content-images\/images\/20241202\/1B5U9LwQaFNSaZ5broITmUQ9jlvdRjLH-31373333303937333539d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
