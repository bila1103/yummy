            },
            {
                "title": "Langkah 6",
                "text": "Lalu tambahkan air.",
                "image_url": "content-images\/images\/20250428\/eX5443a2t73OjCsxBA1HGrmhue60OPHt-31373435383536343731d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
