        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Potong kecil tahu dan tempe, goreng sebentar hingga berkulit.",
                "image_url": "content-images\/images\/20250411\/IhFRBIjwTvkrT7TWylT4TBRclogixJGC-31373434333635313331d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
