        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Haluskan pisang menggunakan garpu. Sisihkan.",
                "image_url": "content-images\/images\/20250507\/rYE79uNk7iRINvUwl2k7d0esMkn6g5EQ-31373436353833343230d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
