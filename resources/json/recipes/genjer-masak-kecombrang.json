            },
            {
                "title": "Langkah 2",
                "text": "Potong-potong oncom Bandung.",
                "image_url": "content-images\/images\/20250305\/wuGyr8P3foSMkFAH05id8iG95REAvcVl-31373431313736373133d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
