            },
            {
                "title": "Langkah 2",
                "text": "Tuang air.",
                "image_url": "content-images\/images\/20250324\/YcBj1IibvlnEOqbWLLoe7ygdyfBpdUXZ-31373432383133313531d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
