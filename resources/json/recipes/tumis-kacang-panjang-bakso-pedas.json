            },
            {
                "title": "Langkah 2",
                "text": "Potong cabe rawit.",
                "image_url": "content-images\/images\/20250426\/QaTiRFikG3GO0Ghbn7MP0XaaY57SFzz5-31373435363631353334d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
