            },
            {
                "title": "Langkah 3",
                "text": "Masukkan ayam suir aduk kembali.",
                "image_url": "content-images\/images\/20250427\/ubpTwmxqLeBoQQiCLc5aYh7EJUaBktsd-31373435373335373136d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
