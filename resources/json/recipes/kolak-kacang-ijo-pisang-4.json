            },
            {
                "title": "Langkah 5",
                "text": "Lalu masukkan pisang",
                "image_url": "content-images\/images\/20250516\/o0WG9nEEVMpmuiQKGk59ifL5yNdYUtOM-31373437333835363531d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
