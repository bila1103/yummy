        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Panaskan mentega tumis bawang bombay sampai transparan.",
                "image_url": "content-images\/images\/20250426\/rXkgL2d7vC5w6QakGF3iinkc1KFzhL8V-31373435363430393234d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
