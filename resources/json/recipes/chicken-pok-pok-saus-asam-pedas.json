        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Potong dadu fillet dada ayam.",
                "image_url": "content-images\/images\/20250410\/IgiNQDIrt7KEj6e3cmLfekRXlhimtCCD-31373434323837383936d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
