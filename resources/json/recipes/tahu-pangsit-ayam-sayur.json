        "cooking_step": [
            {
                "title": "Langkah 1",
                "text": "Haluskan ayam fillet.",
                "image_url": "content-images\/images\/20240614\/rYBqVnCw50WCMA6X4E4nCgsLeLjBG5YB-31373138333439363537d41d8cd98f00b204e9800998ecf8427e.jpg?x-oss-process=image\/resize,w_600,h_600,m_fixed,image\/format,webp",
